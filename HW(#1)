letter=input('Enter a letter: ')
if letter=='a':
    print('The letter is a vowel.')
elif letter=='e':
    print('The letter is a vowel.')
elif letter=='i':
    print('The letter is a vowel.')
elif letter=='o':
    print('The letter is a vowel.')
elif letter=='u':
    print('The letter is a vowel.')
elif letter=='A':
    print('The letter is a vowel.')
elif letter=='E':
    print('The letter is a vowel.')
elif letter=='I':
    print('The letter is a vowel.')
elif letter=='O':
    print('The letter is a vowel.')
elif letter=='U':
    print('The letter is a vowel.')
else: print('The letter is consonant.')
